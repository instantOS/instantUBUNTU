!Lucario Theme
!BLK Cursor
#define lucario_color0        #4f4f4f
#define lucario_color8        #4f4f4f
!RED Tag
#define lucario_color1        #ff6c60
#define lucario_color9        #fa6960
!GRN SpecialKey
#define lucario_color2        #fbb036
#define lucario_color10       #fbb036
!YEL Keyword
#define lucario_color3        #ffffb6
#define lucario_color11       #feffb9
!BLU Number
#define lucario_color4        #5796ed
#define lucario_color12       #6b9fed
!MAG Precondit
#define lucario_color5        #ff73fd
#define lucario_color13       #fc6ffa
!CYN Float
#define lucario_color6        #8ee478
#define lucario_color14       #8ee478
!WHT Search
#define lucario_color7        #eeeeee
#define lucario_color15       #ffffff
!FMT Include, StatusLine, ErrorMsg
#define lucario_colorBD       #ffffff
#define lucario_colorUL       #ffffff
#define lucario_colorIT       #ffffff
!TXT Normal, Normal, Cursor
#define lucario_foreground    #f8f8f2
#define lucario_background    #2b3e50
#define lucario_cursorColor   #e3bf21

